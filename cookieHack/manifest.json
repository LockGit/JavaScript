{
  "name": "Cookie Inject",
  "version": "1.0",
  "manifest_version": 2,
  "description": "A Cookie Hacker Tool By Lock",
  
  "permissions": [
    "activeTab","tabs", "cookies", "http://*/*", "https://*/*"
    ],
  "icons": 
  { 
     "48": "img/icon48.png",
	   "128": "img/icon128.png"
  },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_title": "Cookie Hacker Inject",
	  "default_popup": "popup.html"
  }
}
